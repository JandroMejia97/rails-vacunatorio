<%= form_for (@applied_vaccine) do |f| %>
    <div class="form-group col-12 mb-3">
        <%= f.label :marca, t("vaccines.mark"), class: "form-label" %>
        <%= f.text_field :marca, placeholder: t('vaccines.mark_ph'), class: "form-control" %>
         </div>
    <div class="form-group col-12 mb-3">
        <%= f.label :lot_number, t("vaccines.lot_number"), class: "form-label" %>
        <%= f.text_field :lot_number, placeholder: t('vaccines.lot_number_ph'), class: "form-control" %>
    </div>

    <div class="form-group col-12 mb-3">
        <%= f.label :applied_dose, t("vaccines.applied_dose"), class: "form-label" %>
        <%= f.number_field :applied_dose, placeholder: t('vaccines.applied_dose_ph'), class: "form-control" %>
    </div>

    <%= f.submit button_text, class: "btn btn-primary mx-3", formvalidate: true %>
    <%end%>

